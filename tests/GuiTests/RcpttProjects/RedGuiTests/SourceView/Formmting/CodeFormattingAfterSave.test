        set-caret-pos 17 14
        key-type "NUMPAD_ENTER" "\\u000D"
        type-text Sh
        key-type "M1+SPACE"
        wait 200
        key-type Down -times 3
        key-type Enter
    }
    get-canvas | key-type "TRAVERSE_ESCAPE"
    get-text-viewer | key-type ESC
}

