get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" | double-click

get-editor "TestFile.robot" | get-text-viewer | set-text "*** Settings ***\n\n"
    + "Library           String\n"
    + "Library  DateTime\n\n"
    + "*** Test Cases ***     \n"
    + "test\n"
