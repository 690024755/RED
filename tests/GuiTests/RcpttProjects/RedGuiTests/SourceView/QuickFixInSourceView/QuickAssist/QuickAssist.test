get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table | select "Assign to local scalar" 
    | click -default
    
// changed variable name and add = (checking regions)
with [get-editor "TestFile.robot" | get-text-viewer] {
    type-text "var_3"
    key-type Tab
}
with [get-editor "TestFile.robot" | get-text-viewer] {
    type-text "="
    hover-text 13 9
}

